{
  "manifest_version": 3,
  "name": "Ram Suspender",
  "description": "My custom extension for beta testing. Author: EnderRobber101",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "storage",
    "tabs",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
{
  "manifest_version": 3,
  "name": "EnderRobber101's Preferences",
  "description": "My custom extension for beta testing",
  "version": "0.1",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}